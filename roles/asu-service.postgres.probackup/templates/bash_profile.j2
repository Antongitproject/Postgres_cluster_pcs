[ -f /etc/profile ] && source /etc/profile    
PGDATA={{ PGDATA_PATH }}    
export PGDATA    
[ -f {{ postgres_home_dir }}/.pgsql_profile ] && source /var/lib/pgsql/.pgsql_profile    
BACKUP_PATH={{ DBMS_BACKUP_DIRECTORY }}/pg_probackup    
export BACKUP_PATH    
alias pg_probackup='{{ PGPRO_BACKUP_PATH }}/{{ PGPRO_BACKUP_NAME_UTIL }}'    
